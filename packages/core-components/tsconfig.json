{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": false,
    "declaration": false,
    "experimentalDecorators": true,
    "lib": ["dom", "es2015"],
    "moduleResolution": "node",
    "module": "esnext",
    "target": "es2017",
    "jsx": "react",
    "jsxFactory": "h",
    "strict": true,
    "noUncheckedIndexedAccess": true
  },
  "references": [
    {
      "path": "./tsconfig.lib.json"
    }
  ]
}
